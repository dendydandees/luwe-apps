{
  "env": {
    "browser": true,
    "es2021": true
  },
  "extends": ["airbnb-base"],
  "parserOptions": {
    "ecmaVersion": 12,
    "sourceType": "module"
  },
  "rules": {
    "consistent-return": "off",
    "no-prototype-builtins": "off",
    "no-underscore-dangle": "off",
    "no-restricted-globals": "off",
    "no-param-reassign": "off",
    "no-console": "off",
    "no-undef": "off"
  }
}
